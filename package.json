{
  "name": "desktop",
  "version": "1.0.0",
  "description": "Data Extractor - Professional data collection and restaurant management desktop application",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:vite\" \"wait-on http-get://localhost:5173 && electron .\"",
    "dev:vite": "vite --port 5173 --strictPort",
    "prebuild": "node scripts/generate-icon.cjs && node scripts/prebuild.cjs",
    "build": "npm run prebuild && vite build && electron-builder",
    "build:win": "npm run prebuild && vite build && electron-builder --win --x64",
    "build:dir": "npm run prebuild && vite build && electron-builder --dir",
    "clean": "rimraf dist dist-renderer node_modules/.vite",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "electron",
    "react",
    "data-collection",
    "restaurant-management",
    "data-extraction",
    "automation"
  ],
  "author": {
    "name": "Data Extractor Team",
    "email": "support@dataextractor.com"
  },
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dismissable-layer": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-focus-scope": "^1.1.7",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-presence": "^1.1.5",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "1.1.1",
    "@sentry/electron": "^4.15.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.454.0",
    "puppeteer-core": "^24.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "update-electron-app": "^3.1.1",
    "ws": "^8.18.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "electron": "^38.1.1",
    "electron-builder": "^26.0.12",
    "eslint": "^8.54.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.0",
    "sharp": "^0.34.4",
    "@tailwindcss/postcss": "^4.1.16",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.2",
    "vite": "^5.4.0",
    "vite-plugin-electron": "^0.28.0",
    "wait-on": "^7.2.0"
  },
  "optionalDependencies": {
    "electron-reload": "^2.0.0-alpha.1"
  },
  "build": {
    "appId": "com.dataextractor.desktop",
    "productName": "Data Extractor",
    "copyright": "Copyright Â© 2025 Data Extractor Team",
    "electronDownload": {
      "cache": "./electron-cache"
    },
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "files": [
      "src/**/*",
      "dist-renderer/**/*",
      "assets/icon.ico",
      "package.json",
      "!src/**/*.ts",
      "!**/*.map"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "forceCodeSigning": false,
    "compression": "maximum",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.ico",
      "verifyUpdateCodeSignature": false,
      "signAndEditExecutable": false,
      "artifactName": "${productName}-Setup-${version}-${arch}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Data Extractor",
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "installerHeaderIcon": "assets/icon.ico",
      "deleteAppDataOnUninstall": false,
      "runAfterFinish": true,
      "menuCategory": true,
      "displayLanguageSelector": false
    },
    "publish": null
  }
}
